<script type="text/javascript">
    var username = document.getElementById('username');
    var email = document.getElementById('email');
    var password = document.getElementById('dz-password');
    var submit = document.getElementById('signup');
    submit.addEventListener('click', function () {
        if (email.value.length === 0) {
            errors("Email can't be empty");
        }else if (username.value.length === 0) {
            errors("Username can't be empty");
        } else if (password.value.length === 0) {
            errors("Password can't be empty");
        } else {
            submit.disabled = true;
            submit.innerHTML = '<i class="fa fa-refresh fa-spin"></i>&nbsp;&nbsp; Loading';
            actionRegister(username.value,email.value, password.value, function (response) {
                submit.disabled = false;
                submit.innerHTML = 'Sign Up';
                console.log(response);
                if (response.code == 200) {
                    success(response.message)
                    setTimeout(()=>{
                        window.location.href="./auth";
                    },1500)
                }else{
                    errors(response.message)
                }
                username.value = ""
                email.value = ""
                password.value = ""
            });
            
        }
    });
</script>
